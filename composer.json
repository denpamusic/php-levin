{
    "name": "denpa/php-levin",
    "type": "library",
    "description": "Pure PHP implementation of monero's Levin protocol.",
    "keywords": ["monero","levin","p2p","network"],
    "homepage": "https://github.com/denpamusic/php-levin",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Paavilainen",
            "email": "denpa@netfleet.space",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-sockets": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 | ^8.0",
        "php-vfs/php-vfs": "^1.4"
    },
    "bin": [
        "bin/dummy-node"
    ],
    "autoload": {
        "psr-4" : {
            "Denpa\\Levin\\": "src"
        },
        "files" : [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4" : {
            "Denpa\\Levin\\Tests\\": "tests"
        }
    }
}
